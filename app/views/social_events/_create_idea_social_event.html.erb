<div class="row mobile table-row show-toolbar">
  <div class="two phone-one columns">
    <%= link_to gravatar_for(create_idea_social_event.created_by, :size => 65), create_idea_social_event.created_by %>
  </div>
  <div class="ten phone-three columns">
    <div class="feed-item table-item">
      <strong><a href=""><%= create_idea_social_event.created_by.display_name %></a></strong> has a new idea, to <strong><%= create_idea_social_event.idea.content %></strong>
    </div>
    <section>
      <div class="row">
        <div class="three phone-onw columns">
          <div class="timestamp"><%= time_ago_in_words(create_idea_social_event.updated_at) %></div>
        </div>
        <div class="nine phone-two columns">
          <div class="toolbar right invisible" id="item_toolbar_37">
            <% unless current_user.has_idea? create_idea_social_event.idea %>
              <%= link_to 'Remind me too', new_for_existing_idea_path(create_idea_social_event.idea.id), :remote => true %> ·
            <% end %>
            <span id="good_idea_37">
                <a href="/good-idea/37" data-method="post" data-remote="true" rel="nofollow">Good idea</a> (0)
            </span> ·
            <span id="done_idea_37">
                <a href="/done-idea/37" data-method="post" data-remote="true" rel="nofollow">I did this</a> (0)
            </span>
          </div>
        </div>
      </div>
    </section>
  </div>
</div>